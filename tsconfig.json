{
	"compilerOptions": {
		"target": "ESNEXT",
		"module": "ESNext",
		"sourceMap": true,
		"outDir": ".dist",
		"rootDir": "src",
		"strict": true,
		"moduleResolution": "node",
		"typeRoots": [
			"./node_modules/@types"
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"node_modules",
		".dist"
	]
}